extend type Query {
    memoryUtilizationForApp(
        env: String!
        team: String!
        app: String!
    ) : [ResourceUtilization!]!

    CPUUtilizationForApp(
        env: String!
        team: String!
        app: String!
    ) : [ResourceUtilization!]!
}

type ResourceUtilization {
    timestamp: Time!
    request: Float!
    requestCost: Float!
    usage: Float!
    usageCost: Float!
    requestedFactor: Float!
}