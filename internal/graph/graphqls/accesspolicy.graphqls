type Port {
  port: Int!
}

type External {
  host: String!
  ports: [Port!]!
}

type Rule {
  application: String!
  namespace: String!
  cluster: String!
  mutual: Boolean!
  mutualExplanation: String!
  isJob: Boolean!
}

type Inbound {
  rules: [Rule!]!
}

type Outbound {
  rules: [Rule!]!
  external: [External!]!
}

type AccessPolicy {
  inbound: Inbound!
  outbound: Outbound!
}
