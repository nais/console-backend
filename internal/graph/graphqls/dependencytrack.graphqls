type DependencyTrack {
    projectUUID: String!
    projectName: String!
    findingsLink: String!
    vulnerabilities: [Vulnerability!]
    summary: VulnerabilitySummary!
}

type VulnerabilitySummary {
    total: Int!
    critical: Int!
    high: Int!
    medium: Int!
    low: Int!
    unassigned: Int!
}

type Vulnerability {
    id: String!
    severity: String!
    severityRank: Int!
    name: String!
    componentPurl: String!
}
