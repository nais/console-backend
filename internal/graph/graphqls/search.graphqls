extend type Query {
  search(
    query: String!
    first: Int
    last: Int
    after: Cursor
    before: Cursor
  ): SearchConnection!
}

type SearchConnection {
  edges: [SearchEdge!]!
  pageInfo: PageInfo!
  totalCount: Int!
}

type SearchEdge {
  node: SearchNode!
  cursor: Cursor!
}

union SearchNode = App | Team
