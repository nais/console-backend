type Consume {
    name: String!
}

type Consumer {
    name: String!
    orgno: String!
}

type Expose {
    allowedIntegrations: [String!]!
    atMaxAge: Int!
    consumers: [Consumer!]!
    enabled: Boolean!
    name: String!
    product: String!
}

type MaskinportenScope {
    consumes: [Consume!]!
    exposes: [Expose!]!
}

type Maskinporten {
    scopes: MaskinportenScope!
    enabled: Boolean!
}
