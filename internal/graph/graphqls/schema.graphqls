directive @goField(
  forceResolver: Boolean
  name: String
) on INPUT_FIELD_DEFINITION | FIELD_DEFINITION

interface Node {
  id: ID!
}
type PageInfo {
  hasNextPage: Boolean!
  hasPreviousPage: Boolean!
  startCursor: Cursor
  endCursor: Cursor
  from: Int!
  to: Int!
}

type Query {
  node(id: ID!): Node
}

"The root query for implementing GraphQL mutations."
type Mutation

type Env implements Node {
  id: ID!
  name: String!
}
